<!-- These MUST match the values defined in sip_profile/${profile_name} -->
<X-PRE-PROCESS cmd="set" data="profile_name=sotel-sbc1"/>
<X-PRE-PROCESS cmd="set" data="internal_sip_port=15066"/>
<X-PRE-PROCESS cmd="set" data="external_sip_port=5066"/>

<!-- These are defined here -->
<X-PRE-PROCESS cmd="set" data="ingress_target=sbc-inbound.$${cluster_name}.$${domain_name}"/>
