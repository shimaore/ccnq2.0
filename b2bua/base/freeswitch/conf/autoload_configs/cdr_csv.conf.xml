<configuration name="cdr_csv.conf" description="CDR CSV Format">
  <settings>
    <!-- 'cdr-csv' will always be appended to log-base -->
    <param name="log-base" value="/var/log/freeswitch/cdr_csv"/>
    <param name="default-template" value="ccnq"/>
    <!-- This is like the info app but after the call is hung up -->
    <!--<param name="debug" value="true"/>-->
    <param name="rotate-on-hup" value="true"/>
    <!-- may be a b or ab -->
    <param name="legs" value="ab"/>
  </settings>
  <templates>
    <!-- &#009; is TAB -->
    <template name="ccnq">start=${start_stamp}&#009;account=${ccnq_account}&#009;account_sub=${ccnq_account_sub}&#009;from_e164=${ccnq_from_e164}&#009;to_e164=${ccnq_to_e164}&#009;count=1&#009;duration=${billsec}</template>
  </templates>
</configuration>
