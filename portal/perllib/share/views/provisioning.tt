<% USE loc %>
<% IF session.user_id and result %>
<div class="provisioning">
<% FOREACH r IN result; SET profile = r.profile %>
  <% IF is_admin THEN SET account_uri = FILTER uri %>&account=<% r.account | uri %><% END %><% ELSE SET account_uri = "" END %>
  <h3 class="row_header <% profile %>"><a href="<% prefix %>/provisioning/<% profile | uri %>/<% r.$profile | uri %><%account_uri%>"><% r.$profile %></a></h3>
  <div class="row_content <% profile %>">
    <div>
    <a href="<% prefix %>/provisioning/view/<% profile | uri %>/<% r.$profile | uri %><%account_uri%>">View</a>
    <% IF profile == 'endpoint' and r.password %>
      | <span class="registration"><a href="<% prefix %>/provisioning/endpoint_location?endpoint=<% r.endpoint | uri %><%account_uri%>"><%|loc%>Registration status<%END%></a></span>
    <% END %>
    <% IF profile == 'endpoint' %>
      | <span class="edit"><a href="<% prefix %>/provisioning/endpoint/<% r.endpoint %><%account_uri%>"><%|loc%>Edit<%END%></a></span>
    <% ELSIF profile == 'location' %>
      | <span class="edit"><a href="<% prefix %>/location?location=<% r.location | uri %><%account_uri%>"><%|loc%>Edit<%END%></a></span>
    <% ELSIF profile == 'number' %>
      <% IF r.account %>
      | <span class="edit"><a href="<% prefix %>/number_forwarding/<% r.number %><%account_uri%>"><%|loc%>Change Forwarding<%END%></a></span>
      | <span class="edit"><a href="<% prefix %>/number_location/<% r.number %><%account_uri%>"><%|loc%>Change Location<%END%></a></span>
      | <span class="edit"><a href="<% prefix %>/number_name/<% r.number %><%account_uri%>"><%|loc%>Change Caller Name<%END%></a></span>
      | <span class="edit"><a href="<% prefix %>/numbers/bank/return/<% r.number %><%account_uri%>"><%|loc%>Delete<%END%></a></span>
      <% ELSE %>
      | <span class="edit"><a href="<% prefix %>/numbers/bank/?number=<% r.number %><%account_uri%>"><%|loc%>Modify<%END%></a></span>
      <% END %>
    <% END %>
    </div>
    <% FOREACH d IN r; UNLESS d.key.match('^_|^request|^type|^endpoint_ip|^action') %>
      <div class="entry">
        <span class="key"><%|loc%><% d.key %><%END%></span>
        <span class="value">
          <% IF    d.key == 'endpoint' %><a href="<% prefix %>/provisioning/view/endpoint/<% d.value | uri %><%account_uri%>"><% d.value %></a>
          <% ELSIF d.key == 'location' %><a href="<% prefix %>/provisioning/view/location/<% d.value | uri %><%account_uri%>"><% d.value %></a>
          <% ELSIF d.key == 'account' %>
            <form action="<% prefix %>/account" method="POST" accept-charset="utf-8" class="ui-widget">
              <input type="submit" name="account" value="<% d.value | html %>">
            </form>
          <% ELSE %><% d.value %><% END %>
        </span>
      </div>
    <% END; END %>
  </div>
<% END %>
</div>
<% ELSE %>
  <% IF session.user_id %>
    <div class="ui-widget">
    <% | loc %>No results. Maybe you need to select an account?<% END %>
    </div>
  <% ELSE %>
    <div class="ui-widget">
    <% | loc %>Please log in.<% END %>
    </div>
  <% END %>
<% END %>
