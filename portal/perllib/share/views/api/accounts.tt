<% USE loc %>
<table>
<% FOREACH doc IN all_accounts() %>
  <tr>
    <td>
      <form action="<% prefix %>/account" method="POST" accept-charset="utf-8" class="ui-widget">
        <input type="submit" name="account" value="<% session.account | html %>">
      </form>
    </td>
    <td><% doc.name | html %></td>
  </tr>
<% END %>
</table>
